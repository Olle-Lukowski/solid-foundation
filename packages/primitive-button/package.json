    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "solid-js": "^1.9.9"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/jest-dom": "^6.8.0",
    "jsdom": "^26.1.0",
    "solid-js": "^1.9.9",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
