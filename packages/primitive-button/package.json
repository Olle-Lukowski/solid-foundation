{
  "name": "@solid-foundation/primitive-button",
  "version": "0.1.0",
  "type": "module",
  "description": "A primitive button component for Solid Foundation - a simple wrapper around native HTML button with accessibility features",
  "author": "Olle Lukowski <olle@lukowski.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Olle-Lukowski/solid-foundation.git",
    "directory": "packages/primitive-button"
  },
  "homepage": "https://github.com/Olle-Lukowski/solid-foundation/tree/main/packages/primitive-button",
  "bugs": {
    "url": "https://github.com/Olle-Lukowski/solid-foundation/issues"
  },
  "keywords": [
    "solidjs",
    "button",
    "primitive",
    "component",
    "ui",
    "accessibility"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
